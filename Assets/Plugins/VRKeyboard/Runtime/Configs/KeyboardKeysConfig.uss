Label {
    font-size: 20px;
    -unity-font-style: bold;
    color: rgb(68, 138, 255);
}

Row {
    flex-direction: row;
    background-color: rgb(40, 40, 40);
}

Key {
    width: 40px;
    height: 40px;
    border-radius: 5px;
    background-color: rgb(80, 80, 80);
}

KeyTextField {
    width: 40px;
    height: 40px;
    margin: 0px;
    position: absolute;
}

TextInput {
    background-color: rgb(120, 120, 120);
    -unity-text-align: middle-center;
}